@model List<BaiTap.Models.TonKho>

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Danh sách tồn kho</title>
</head>
<body>
    <h2>Danh sách tồn kho</h2>
    <table>
        <thead>
            <tr>
                <th>Tên sản phẩm</th>
                <th>Số lượng</th>
                <th>Ngày cập nhật</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.SanPham.TenSanPham</td>
                    <td>@item.SoLuongTon</td>
                    <td>@item.NgayCapNhat.ToString()</td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>
